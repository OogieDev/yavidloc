                                                {% for child in category.children %}
                                                    <li class="navbar__subcatalog-item"><a href="{{ child.href }}">{{ child.name }}</a></li>
                                                {% endfor %}
                                            </ul>
                                        {% else %}
                                            <li class="navbar__catalog-item"><a class="navbar__catalog-link" href="{{ category.href }}">{{ category.name }}</a></li>
